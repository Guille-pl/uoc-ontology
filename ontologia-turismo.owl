<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/guill/ontologies/2025/tourism/"
     xml:base="http://www.semanticweb.org/guill/ontologies/2025/tourism/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:tourism="http://www.semanticweb.org/guill/ontologies/2025/tourism#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hasAttraction -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hasAttraction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:comment xml:lang="en">Relates a city to an attraction located within it.</rdfs:comment>
        <rdfs:label xml:lang="en">has Attraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hasEvent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hasEvent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Event"/>
        <rdfs:comment xml:lang="en">Indicates that a city or destination hosts a specific event.</rdfs:comment>
        <rdfs:label xml:lang="en">has Event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hasHotel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hasHotel">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
        <rdfs:comment xml:lang="en">Indicates that a city or location offers or contains a hotel.</rdfs:comment>
        <rdfs:label xml:lang="en">has Hotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hasRestaurant -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hasRestaurant">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">Indicates that a city or location offers or contains a restaurant.</rdfs:comment>
        <rdfs:label xml:lang="en">has Restaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hasTransport -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hasTransport">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Transport"/>
        <rdfs:comment xml:lang="en">Indicates that a city or area provides access to a means of transportation.</rdfs:comment>
        <rdfs:label xml:lang="en">has Transport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Price -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Price">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Transport"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Price"/>
        <rdfs:comment xml:lang="en">Relates a tourism service or event to its price.</rdfs:comment>
        <rdfs:label xml:lang="en">has Price</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Rating -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Rating">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Rating"/>
        <rdfs:comment xml:lang="en">Relates a tourism resource to its user-assigned or system-generated rating.</rdfs:comment>
        <rdfs:label xml:lang="en">has Rating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#attractionName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#attractionName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The name of a tourist attraction, such as a museum, monument, or natural site.</rdfs:comment>
        <rdfs:label xml:lang="en">attraction name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#cityLocation -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#cityLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Provides the geographic location or coordinates of the city, such as latitude and longitude.</rdfs:comment>
        <rdfs:label xml:lang="en">city location</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#cityName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#cityName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The name of a city or urban location relevant to the tourism ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">city name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#eventName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#eventName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Specifies the name or title of an event offered at a tourist destination.</rdfs:comment>
        <rdfs:label xml:lang="en">event name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#hotelName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#hotelName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The official or commercial name of a hotel establishment.</rdfs:comment>
        <rdfs:label xml:lang="en">hotel name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#priceValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#priceValue">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Price"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Represents the numerical cost associated with a tourism-related service, such as a hotel, restaurant, or event.</rdfs:comment>
        <rdfs:label xml:lang="en">price value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#ratingValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#ratingValue">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Rating"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Represents the numerical rating given to a tourism resource, usually on a scale (e.g., 1 to 5).</rdfs:comment>
        <rdfs:label xml:lang="en">rating value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#restaurantName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#restaurantName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The official name or brand of a restaurant establishment.</rdfs:comment>
        <rdfs:label xml:lang="en">restaurant name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#transportType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#transportType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Indicates the type of transportation, such as bus, train, taxi, or metro.</rdfs:comment>
        <rdfs:label xml:lang="en">transport type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#AndalusianRestaurant -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#AndalusianRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">A restaurant specializing in cuisine from the Andalusian region of Spain.</rdfs:comment>
        <rdfs:label xml:lang="en">Andalusian Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Rating"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Rating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A place or activity that draws tourists due to its cultural, historical, natural, or recreational value.</rdfs:comment>
        <rdfs:label xml:lang="en">Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Average -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Average">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Calification"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Bad -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Bad">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Calification"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#BeachAttraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#BeachAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:comment xml:lang="en">A coastal area known for its natural beauty, sand, and recreational water activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Beach Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#BeachHotel -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
        <rdfs:comment xml:lang="en">A hotel located near or on a beach, catering to tourists seeking coastal experiences.</rdfs:comment>
        <rdfs:label xml:lang="en">Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#BusinessHotel -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#BusinessHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
        <rdfs:comment xml:lang="en">A hotel offering services tailored to business travelers, including meeting rooms and Wi-Fi.</rdfs:comment>
        <rdfs:label xml:lang="en">Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Calification -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Calification"/>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#City -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#City">
        <rdfs:comment xml:lang="en">A large human settlement that serves as a central point for tourism, including attractions, accommodation, and transport.</rdfs:comment>
        <rdfs:label xml:lang="en">City</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#CulturalAttraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#CulturalAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:comment xml:lang="en">A tourist site of cultural significance, including traditions, arts, or heritage.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#CulturalEvent -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#CulturalEvent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Event"/>
        <rdfs:comment xml:lang="en">A public occasion celebrating or showcasing cultural expressions and traditions.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Event -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Event">
        <rdfs:comment xml:lang="en">A planned public or social occasion that can be part of a touristâ€™s itinerary.</rdfs:comment>
        <rdfs:label xml:lang="en">Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Excelent -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Excelent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Calification"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#GastronomicEvent -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#GastronomicEvent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Event"/>
        <rdfs:comment xml:lang="en">A food-related event showcasing regional or international cuisine and culinary practices.</rdfs:comment>
        <rdfs:label xml:lang="en">Gastronomic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Price"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Price"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#has_Rating"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Rating"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A commercial establishment offering lodging, meals, and other guest services for travelers and tourists.</rdfs:comment>
        <rdfs:label xml:lang="en">Hotel</rdfs:label>
        <rdfs:sameAs xml:lang="en">https://dbpedia.org/page/Hotel</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#ItalianRestaurant -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#ItalianRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">A restaurant focused on serving Italian cuisine such as pasta, pizza, and regional specialties.</rdfs:comment>
        <rdfs:label xml:lang="en">Italian Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#JapaneseRestaurant -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#JapaneseRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">A restaurant offering dishes and food traditions typical of Japanese culture.</rdfs:comment>
        <rdfs:label xml:lang="en">Japanese Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#LuxuryHotel -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#LuxuryHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Hotel"/>
        <rdfs:comment xml:lang="en">A high-end hotel offering premium services and amenities for upscale travelers.</rdfs:comment>
        <rdfs:label xml:lang="en">Luxury Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#MediterraneanRestaurant -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#MediterraneanRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">A restaurant that serves cuisine inspired by the countries bordering the Mediterranean Sea.</rdfs:comment>
        <rdfs:label xml:lang="en">Mediterranean Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#MexicanRestaurant -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#MexicanRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Restaurant"/>
        <rdfs:comment xml:lang="en">A restaurant offering traditional dishes from Mexican culinary culture.</rdfs:comment>
        <rdfs:label xml:lang="en">Mexican Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#MonumentAttraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#MonumentAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:comment xml:lang="en">A historically significant structure or landmark that attracts visitors.</rdfs:comment>
        <rdfs:label xml:lang="en">Monument Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#MuseumAttraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/guill/ontologies/2025/tourism#MuseumAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/guill/ontologies/2025/tourism#Attraction"/>
        <rdfs:comment xml:lang="en">A location that displays collections of artistic, historical, or scientific interest.</rdfs:comment>
        <rdfs:label xml:lang="en">Museum Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/guill/ontologies/2025/tourism#MusicalEvent -->

